<Window x:Class="Cs2_WPF_Tanaeva.EditDep"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cs2_WPF_Tanaeva"
        mc:Ignorable="d"
        Title="EditDep" Height="450" Width="800">
    <Grid Margin="0,0,431,367">
        <TextBox Name="tb_DepName" Height="23" Margin="163,10,0,0" TextWrapping="Wrap" Text="{Binding depName}" VerticalAlignment="Top" />
        <Label Content="Введите новое название:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
